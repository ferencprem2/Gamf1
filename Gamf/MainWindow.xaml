<Window x:Class="Gamf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gamf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="Button_Click" Width="100" Height="100">Szamol</Button>
        <Label x:Name="firstLabel" Width="100" Height="100" Margin="0 -250 -200 0"></Label>
        <Label x:Name="timeLabel" Width="150" Height="100" Margin="0 -250 200 0"></Label>
    </Grid>
</Window>
